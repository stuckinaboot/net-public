{
  "name": "net-cli-storage-upload",
  "version": "1.0.0",
  "description": "CLI tool for uploading files to Net Storage",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "net-storage-upload": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@net-protocol/core": "*",
    "@net-protocol/storage": "*",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "fs-extra": "^11.2.0",
    "viem": "^2.21.45"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.6",
    "typescript": "^5.6.3",
    "vitest": "^1.0.0"
  }
}
