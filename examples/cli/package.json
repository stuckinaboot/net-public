{
  "name": "net-cli",
  "version": "1.0.0",
  "description": "CLI tool for Net Protocol",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "bin": "./src/cli/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "yarn tsx src/cli/index.ts",
    "dev": "yarn tsx watch src/cli/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@net-protocol/core": "*",
    "@net-protocol/storage": "*",
    "@x402/evm": "^2.1.0",
    "@x402/fetch": "^2.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "viem": "^2.21.45"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vitest": "^1.0.0"
  }
}
