{
  "name": "@net-protocol/cli",
  "version": "0.1.6",
  "description": "CLI tool for Net Protocol",
  "type": "module",
  "bin": {
    "netp": "./dist/cli/index.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "clean": "rm -rf dist",
    "dev": "tsx watch src/cli/index.ts",
    "start": "tsx src/cli/index.ts",
    "typecheck": "tsc --noEmit",
    "prepack": "../../scripts/prepack-modify-deps.sh",
    "prepublishOnly": "yarn build",
    "postpublish": "../../scripts/postpublish-restore-deps.sh",
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@net-protocol/core": "^0.1.6",
    "@net-protocol/netr": "^0.1.3",
    "@net-protocol/profiles": "file:../net-profiles",
    "@net-protocol/relay": "^0.1.3",
    "@net-protocol/storage": "^0.1.11",
    "@x402/evm": "^2.1.0",
    "@x402/fetch": "^2.1.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^17.2.3",
    "viem": "^2.31.4"
  },
  "devDependencies": {
    "@types/node": "^20.17.6",
    "@vitest/ui": "^1.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vitest": "^1.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stuckinaboot/net-public.git",
    "directory": "packages/net-cli"
  },
  "keywords": [
    "net",
    "protocol",
    "blockchain",
    "ethereum",
    "cli",
    "storage"
  ],
  "license": "MIT"
}
